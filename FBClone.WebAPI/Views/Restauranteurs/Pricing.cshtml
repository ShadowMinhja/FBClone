@{
    Layout = null;
    ViewBag.Title = "fbClone | Pricing";
}

<!DOCTYPE html>
<html>
<head>

    <meta http-equiv="content-type" content="text/html; charset=utf-8" />





    <!-- ============================================
    ================= Stylesheets ===================
    ============================================= -->

    <link href="http://fonts.googleapis.com/css?family=Lato:300,400,400italic,700,700italic|Raleway:300,400,500,600,700" rel="stylesheet" type="text/css" />

    <!-- ============================================
    ============= Main App Stylesheet ===============
    ============================================= -->

    <link rel="stylesheet" href="/corporate/assets/css/bundle.min.css" type="text/css" />

    <!-- jQuery -->
    <script type="text/javascript" src="/corporate/assets/js/vendor/jquery-1.11.2.min.js"></script>






    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <!--[if lt IE 9]>
        <script src="http://css3-mediaqueries-js.googlecode.com/svn/trunk/css3-mediaqueries.js"></script>
    <![endif]-->
    <!-- ============================================
    ================== Page Title ===================
    ============================================= -->

    <title>@ViewBag.Title</title>







</head>

<body>








    <!-- ============================================
    ================= Page Wrapper ==================
    ============================================= -->

    <div id="wrapper" class="clearfix animsition">







        <!-- ================================================
        ================= Search Container ==================
        ================================================= -->

        <div id="search-container" class="search-box-wrapper">
            <div class="container">
                <i class="fa fa-search"></i>
                <div class="search-box">
                    <form action="http://example.com/" class="search-form" role="search">
                        <input type="search" name="s" value="" title="Press Enter to submit your search" placeholder="Search…" class="search-field">
                        <input type="submit" value="Search" class="search-submit">
                    </form>
                </div>
            </div>
        </div><!--/ #search-container -->
        <!-- ============================================
        ==================== Header =====================
        ============================================= -->

        <header id="header" class="transparent-header dark">
            <!-- class .sticky-mobile makes header sticky on small devices -->

            <div id="header-wrap">

                <div class="container clearfix">

                    <div id="main-navbar-toggle"><i class="fa fa-bars"></i></div>







                    <!-- ============================================
                    =================== Branding ====================
                    ============================================= -->

                    @Html.Partial("_BrandingPartial")
                    <!-- ============================================
                    ================= Main Navbar ===================
                    ============================================= -->

                    @Html.Partial("_MenuPartial")

                </div>

            </div>

        </header><!-- #header end -->
        <!-- ============================================
        =================== Breadcrumbs =================
        ============================================= -->
        <section id="breadcrumbs">

            <div class="container clearfix">
                <h1>Prices</h1>
                <span>Our subscription plans special for you</span>
                <ol class="breadcrumb">
                    <li><a href="/">Home</a></li>
                    <li><a href="#">Pricing</a></li>
                    <li class="active">Prices</li>
                </ol>
            </div>

        </section><!-- #breadcrumbs end -->
        <!-- ============================================
        =================== Content =====================
        ============================================= -->

        <section id="content">

            <div class="content-wrap">



                <!-- ============ three tables section ============ -->
                <div class="section m-0 p-0">
                    <div class="container clearfix">

                        <div class="heading-block mb-20">
                            <h3 class="text-uppercase mb-0"><span class="text-theme">Our</span> Offerings</h3>
                        </div>


                        <div class="row price-plans">


                            <div class="col-sm-4">

                                <div class="price-plan">

                                    <!-- plan type -->
                                    <div class="plan-type">
                                        <h4><strong>FREE</strong> Plan</h4>
                                    </div>
                                    <!-- end plan type -->
                                    <!-- plan pricing -->
                                    <div class="plan-price">
                                        <span class="unit">$</span> <span class="price">FREE</span><span class="period">/ mo</span>
                                    </div>
                                    <!-- end plan pricing -->
                                    <!-- plan features -->
                                    <div class="plan-features">
                                        <ul>
                                            <li><strong>Free</strong> Android/iOS app download</li>
                                            <li><strong>Free</strong> Access to analytics dashboard</li>
                                            <li><strong>Free</strong> Customer service trends charts</li>
                                            <li>Limit <strong>1</strong> Questionnaire</li>
                                        </ul>
                                    </div>
                                    <!-- end plan-features -->
                                    <!-- plan actions -->
                                    <div class="plan-actions">
                                        <a class="myBtn myBtn-rounded myBtn-lightred" href="/app/#/signup">Sign Up</a>
                                    </div>
                                    <!-- end plan actions -->

                                </div>

                            </div>

                            <div class="col-sm-4">

                                <div class="price-plan selected">

                                    <!-- plan type -->
                                    <div class="plan-type">
                                        <h4><strong>Premium</strong> Plan</h4>
                                        <span>(Coming Soon)</span>
                                    </div>
                                    <!-- end plan type -->
                                    <!-- plan pricing -->
                                    <div class="plan-price">
                                        <span class="unit">$</span> <span class="price">39</span><span>.99</span> <span class="period">/ mo</span>
                                    </div>
                                    <!-- end plan pricing -->
                                    <!-- plan features -->
                                    <div class="plan-features">
                                        <ul>
                                            <li><strong>Free</strong> Android/iOS app download</li>
                                            <li><strong>Free</strong> Access to analytics dashboard</li>
                                            <li><strong>Free</strong> Customer service trends charts</li>
                                            <li>Limit <strong>5</strong> Questionnaires</li>
                                            <li><strong>Menu Management</strong> Included</li>
                                        </ul>
                                    </div>
                                    <!-- end plan-features -->
                                    <!-- plan actions -->
                                    <div class="plan-actions">
                                        <a class="myBtn myBtn-rounded myBtn-midlight" href="#" disabled>Free 60-Day Trial</a>
                                        <h6>No commitment - No credit card required</h6>
                                    </div>
                                    <!-- end plan actions -->

                                </div>

                            </div>

                            <div class="col-sm-4">

                                <div class="price-plan">

                                    <!-- plan type -->
                                    <div class="plan-type">
                                        <h4><strong>Professional</strong> Plan</h4>
                                        <span>(Coming Soon)</span>
                                    </div>
                                    <!-- end plan type -->
                                    <!-- plan pricing -->
                                    <div class="plan-price">
                                        <span class="unit">$</span> <span class="price">99</span><span>.99</span> <span class="period">/ mo</span>
                                    </div>
                                    <!-- end plan pricing -->
                                    <!-- plan features -->
                                    <div class="plan-features">
                                        <ul>
                                            <li><strong>Free</strong> Android/iOS app download</li>
                                            <li><strong>Free</strong> Access to analytics dashboard</li>
                                            <li><strong>Free</strong> Customer service trends charts</li>
                                            <li><strong>Unlimited</strong> Questionnaires</li>
                                            <li><strong>Menu Management</strong> Included</li>
                                            <li><strong>Machine Learning Analysis</strong> Included</li>
                                            <li>Free Support</li>
                                        </ul>
                                    </div>
                                    <!-- end plan-features -->
                                    <!-- plan actions -->
                                    <div class="plan-actions">
                                        <a class="myBtn myBtn-rounded myBtn-midlight" href="#" disabled>Free 60-Day Trial</a>
                                        <h6>No commitment - No credit card required</h6>
                                    </div>
                                    <!-- end plan actions -->

                                </div>

                            </div>




                        </div>


                    </div>
                </div><!-- /three tables section -->

                <!-- ============ comparison table section ============ -->
                <div class="section m-0 pt-0">
                    <div class="container clearfix">

                        <div class="heading-block mb-20">
                            <h3 class="text-uppercase mb-0"><span class="text-theme">Comparison</span> Table</h3>
                        </div>


                        <div class="table-responsive">


                            <table class="table">
                                <thead>
                                    <tr>
                                        <th></th>
                                        <th class="text-center">Free Plan</th>
                                        <th class="text-center">Premium Plan</th>
                                        <th class="text-center">Professional Plan</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td><strong>Android/iOS Mobile App</strong></td>
                                        <td class="text-center"><i class="fa fa-check text-greensea"></i></td>
                                        <td class="text-center"><i class="fa fa-check text-greensea"></i></td>
                                        <td class="text-center"><i class="fa fa-check text-greensea"></i></td>
                                    </tr>
                                    <tr>
                                        <td><strong>Access to analytics dashboard</strong></td>
                                        <td class="text-center"><i class="fa fa-check text-greensea"></i></td>
                                        <td class="text-center"><i class="fa fa-check text-greensea"></i></td>
                                        <td class="text-center"><i class="fa fa-check text-greensea"></i></td>
                                    </tr>
                                    <tr>
                                        <td><strong>Customer service trends charts</strong></td>
                                        <td class="text-center"><i class="fa fa-check text-greensea"></i></td>
                                        <td class="text-center"><i class="fa fa-check text-greensea"></i></td>
                                        <td class="text-center"><i class="fa fa-check text-greensea"></i></td>
                                    </tr>
                                    <tr>
                                        <td><strong>No. Questionnaires Limit</strong></td>
                                        <td class="text-center">1</td>
                                        <td class="text-center">5</td>
                                        <td class="text-center">Unlimited</td>
                                    </tr>
                                    <tr>
                                        <td><strong>Menu Management</strong></td>
                                        <td class="text-center"><i class="fa fa-times text-lightred"></i></td>
                                        <td class="text-center"><i class="fa fa-check text-greensea"></i></td>
                                        <td class="text-center"><i class="fa fa-check text-greensea"></i></td>
                                    </tr>
                                    <tr>
                                        <td><strong>Machine Learning Analysis</strong></td>
                                        <td class="text-center"><i class="fa fa-times text-lightred"></i></td>
                                        <td class="text-center"><i class="fa fa-times text-lightred"></i></td>
                                        <td class="text-center"><i class="fa fa-check text-greensea"></i></td>
                                    </tr>
                                    <tr>
                                        <td><strong>Free Support</strong></td>
                                        <td class="text-center"><i class="fa fa-times text-lightred"></i></td>
                                        <td class="text-center"><i class="fa fa-times text-lightred"></i></td>
                                        <td class="text-center"><i class="fa fa-check text-greensea"></i></td>
                                    </tr>
                                    <tr>
                                        <td><strong>Monthly Price</strong></td>
                                        <td class="text-center">Free</td>
                                        <td class="text-center">$39.99</td>
                                        <td class="text-center">$99.99</td>
                                    </tr>
                                    <tr>
                                        <td><strong>Quarterly Price</strong></td>
                                        <td class="text-center">Free</td>
                                        <td class="text-center">$119.99</td>
                                        <td class="text-center">$299.99</td>
                                    </tr>
                                    <tr>
                                        <td><strong>Yearly Price</strong></td>
                                        <td class="text-center">Free</td>
                                        <td class="text-center">$479.99</td>
                                        <td class="text-center">$1,199.99</td>
                                    </tr>
                                    <tr>
                                        <td></td>
                                        <td class="text-center"><a href="/app/#/signup" class="myBtn myBtn-rounded myBtn-border myBtn-theme myBtn-sm m-10">Sign Up</a></td>
                                        <td class="text-center" colspan="2">
                                            <a href="" class="myBtn myBtn-rounded myBtn-border myBtn-theme myBtn-sm m-10">Free 60-Day Trial</a><br />
                                            No commitment - No credit card required
                                        </td>
                                        @*<td class="text-center"><a href="" class="myBtn myBtn-rounded myBtn-border myBtn-theme myBtn-sm m-10">Buy Now</a></td>*@
                                    </tr>
                                </tbody>
                            </table>


                        </div>



                    </div>
                </div><!-- /comparison table section -->






            </div>
        </section><!-- #content end -->
        <!-- ============================================
        ==================== Footer =====================
        ============================================= -->
        <footer id="footer">
            @Html.Partial("_FooterPartial")
        </footer><!-- #footer end -->

    </div><!-- #wrapper end -->
    <!-- ============================================
    =================== Go to Top ===================
    ============================================= -->

    <div id="gotoTop" class="fa fa-angle-up hidden-md"></div>

    <script type="text/javascript" src="/corporate/assets/js/bundle.min.js"></script>


</body>
</html>