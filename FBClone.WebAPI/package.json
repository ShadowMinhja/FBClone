{
  "name": "fbClone",
  "version": "1.0.0",
  "scripts": {
    "setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
    "remove-demo": "babel-node Scripts/tools/removeDemo.js",
    "start-message": "babel-node Scripts/tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node Scripts/tools/srcServer.js",
    "open:dist": "babel-node Scripts/tools/distServer.js",
    "lint": "esw webpack.config.* Scripts/app Scripts/tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir Scripts/app/dist",
    "remove-dist": "rimraf ./Scripts/app/dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html && npm run lint && npm run test",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "mocha Scripts/tools/testSetup.js \"Scripts/app/**/*.spec.js\" --reporter progress",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root Scripts/app --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"Scripts/app/**/*.spec.js\" --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root Scripts/app --report lcovonly _mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "babel-polyfill": "^6.7.4",
    "bootstrap": "4.0.0-alpha.5",
    "material-ui": "0.16.4",
    "object-assign": "4.1.0",
    "react": "15.4.0",
    "react-dom": "15.4.0",
    "react-joyride": "1.4.7",
    "react-materialui-notifications": "0.3.2",
    "react-redux": "4.4.5",
    "react-router": "3.0.0",
    "react-tap-event-plugin": "2.0.1",
    "react-tooltip": "3.0.13",
    "redux": "3.6.0",
    "redux-thunk": "2.0.1",
    "superagent": "2.0.0",
    "rc-upload": "2.1.1",
    "react-dropzone": "3.6.0",
    "react-toastr": "2.8.1",
    "react-image-gallery": "0.7.2"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.8.0",
    "browser-sync": "2.12.5",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.20.0",
    "connect-history-api-fallback": "1.2.0",
    "coveralls": "2.11.9",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "eslint": "2.9.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-jsx-a11y": "1.0.4",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "grunt": "0.4.5",
    "grunt-autoprefixer": "^0.7.3",
    "grunt-bower-task": "0.4.0",
    "grunt-concurrent": "^0.5.0",
    "grunt-contrib-clean": "^0.5.0",
    "grunt-contrib-concat": "^0.4.0",
    "grunt-contrib-connect": "^0.7.1",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-contrib-cssmin": "^0.9.0",
    "grunt-contrib-htmlmin": "^0.3.0",
    "grunt-contrib-imagemin": "^1.0.1",
    "grunt-contrib-jshint": "^0.10.0",
    "grunt-contrib-sass": "^0.7.3",
    "grunt-contrib-uglify": "^0.4.0",
    "grunt-contrib-watch": "^0.6.1",
